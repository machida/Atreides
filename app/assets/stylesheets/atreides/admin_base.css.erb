  font-size:3em;
  text-align:center;
  font-style:italic;
  font-weight:bold;
  color:#ddd;
  margin:0;
}

form.formtastic fieldset.actions,
fieldset.actions {
  padding:0;
  width:97.5%;
}

form.formtastic fieldset.actions li,
fieldset.actions li {
  float:left;
  width:auto;
  padding:0;
  margin-right:0.5em
}

form.formtastic fieldset.actions li.cancel,
form.formtastic fieldset.actions li.delete,
fieldset.actions li.cancel,
fieldset.actions li.delete {
  float:right;
}

fieldset.actions input,
fieldset.actions button,
fieldset.actions input:not([type="checkbox"]):not([type="radio"]):not([type="file"]),
fieldset.actions a {
  background:#eee;
  border:1px solid #aaa;
  padding:10px;
  font-weight:bold;
  font-size:1.2em;
  cursor:pointer;
  -moz-border-radius: 3px;
  -webkit-border-radius: 3px;
  border-radius: 3px;
  display:block;
  box-shadow:none;
}


fieldset.actions button[type="submit"] {
  color:#5D9A25;
}

fieldset.actions .cancel {
  text-decoration:none;
  color:#8A1F11;
}

fieldset.actions .commit a,
fieldset.actions .delete a {
  border-color:#fff;
  background:none;
  font-weight:normal;
  color:#999;
  text-decoration:none;
}

fieldset.actions .delete a:hover {
  background:#8A1F11;
  color:#fff;
  border:1px solid #000;
}

fieldset.actions .cancel:hover {
  background:#FBE3E5;
  border-color:#F5C0C3;
}

fieldset.actions input:not([type="checkbox"]):not([type="radio"]):not([type="file"]).create:hover,
fieldset.actions input:not([type="checkbox"]):not([type="radio"]):not([type="file"]).update:hover,
fieldset.actions .commit a:hover {
  background:#E6F0C2;
  border-color:#C6D880;
  color:#5D9A25;

}

ul.ui-autocomplete {
  width:195px;
  margin:0;
  padding:0;
  list-style:none;
}

ul.ui-autocomplete li {
  padding: 3px 5px;
}

ul.ui-autocomplete a {
  display:block;
}

ul.ui-autocomplete a:hover {
  background:#eee;
  border:none;
  font-weight:normal;
  color:inherit;

}

#ui-datepicker-div {
  display:none;
}

/*
 * Validation
 *
 */

#errorExplanation {
  margin: 0 10px;
}

#errorExplanation h2 {
  font-size:1.5em;
  margin-bottom:0.25em;
  color:#8A1F11;
}

#errorExplanation p {
  font-size:1.1em;
}

#errorExplanation ul {
  list-style:disc inside;
  padding:0;
  margin:0.5em;
}

form.formtastic fieldset ol li.error {
  border:none;
  background:inherit;
}

form.formtastic fieldset ol li.error label {
  color:#8A1F11;
}

form.formtastic fieldset ol li.error input {
  border-color: #8A1F11;
}

form.formtastic fieldset ol li p.inline-errors {
  margin-left:0;
  background:#8A1F11;
  color:#fff;
  padding:0.2em 0.5em;
