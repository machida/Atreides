/*
 * Atreides - Admin Styles
 *
 * Lachlan Laycock @ Jackson Laycock
 * 22 Feb 2010
 *
 * DO NOT EDIT THIS FILE!
 * CHANGES TO THIS FILE WILL BE LOST!
 *
 * Use admin.css for location modifactions
 *
 */

/*
 * Common
 *
 */

body {
  background: #0A0B0C url("<%= image_path('atreides/admin/background.png') %>") 0 0 repeat-x;
}

a {
  outline:none;
  color:#1DAAC6;
}

a:hover {
  text-decoration:underline;
}

thead th {
  background:none;
  border-bottom:1px solid #ccc;
}

tbody td,
tbody tr.even td,
tbody tr.odd td {
  border-bottom:1px solid #eee;
  background:none;
  padding:6px;
}

tbody tr:hover td {
  background:#f5f5f5;
}

tbody tr td:first-child a {
  font-weight:bold;
  color:#333;
  text-decoration:none;
}

tbody tr td:first-child a:hover {
  text-decoration:underline;
}

tfoot td {
  border-bottom:1px solid #ccc;
  font-weight:bold;
}

.flash {
  -moz-border-radius: 3px;
  -webkit-border-radius: 3px;
  border-radius: 3px;
  border-width:1px;
}

.left {
  float:left;
}

.right {
  float:right;
}

/*
 * Header
 *
 */


#header {
  margin-top:1em;
  height:70px;
}

#header h1,
#header h1 a {
  display:block;
  height:61px;
  overflow:hidden;
  text-indent:-9000px;
  background: transparent url("<%= image_path('atreides/admin/header.png') %>") 0 0 no-repeat;
  margin-bottom:0;
}

#nav {
  float:right;
  color:#fff;
}

#nav ul {
  list-style:none;
}

#nav li {
  float:left;
  margin-top:1em;
  margin-right:1em;
  font-size:1.2em;
  line-height:1.9em;
}

#nav ul,
#nav li:last-child {
  margin-right:0;
}

#nav a {
  color:#fff;
}

#admin_search a {
  display:block;
  width:28px;
  height:20px;
  position:absolute;
  margin:3px 0 5px 8px;
}

#admin_search input,
#site_selector {
  border:1px solid #ccc;
  -moz-border-radius: 3px;
  -webkit-border-radius: 3px;
  padding:3px;
  padding-left:30px;
  margin:0;
  font-size:1em;
  width:100px;
  background: url("<%= image_path('atreides/admin/input_bg.gif') %>") repeat-x scroll 0 0 #FFFFFF;
}

#site_selector {
  padding:0 5px;
  line-height:25px;
  color:#666666;
}

#site_selector label {
  border-right:1px solid #ccc;
  margin-right:5px;
  font-weight:normal;
  color:#95A3AA;
}

#site_selector span {
  line-height:25px;
  background: #fff url("<%= image_path('atreides/admin/select_arrows.png') %>") 100% 50% no-repeat;
  padding-right:15px;
}

#site_selector .sites {
  position:absolute;
  display:none;
  border:none;
  background: #fff url("<%= image_path('atreides/admin/input_bg.gif') %>") repeat-x scroll 0 0;
  width:75px;
  margin-top:-26px;
  margin-left:29px;
  border:1px solid #ccc;
  -moz-border-radius: 3px;
  -webkit-border-radius: 3px;
  -webkit-box-shadow: #000 0px 1px 3px;
  -moz-box-shadow: #999 0px 2px 4px;
  box-shadow: #000 0px 1px 3px;
  z-index:1;
  opacity:0.95;
}

#site_selector:hover .sites {
  display:block;
}

#site_selector ol,
#site_selector li {
  margin:0;
  padding:0;
  list-style:none;
  float:none;
  line-height:1em;
  font-size:1em;
}

#site_selector li {
  padding:5px;
}

#site_selector a,
#site_selector li span {
  color:#666666;
  text-decoration:none;
  display:block;
  background:none;
  padding:0;
  margin:0;
}

#site_selector li span {
  line-height:1em;
}

#site_selector li {
  border-bottom:1px solid #eee;
}

#site_selector li:last-child {
  border-bottom:none;
}

/*
 * Body
 *
 */


#body {
  background: #fff url("<%= image_path('atreides/admin/content_bg_edit_form.png') %>") 0 0;
  -moz-border-radius: 12px;
  -webkit-border-radius: 12px;
  border-radius: 12px;
}

#body.dashboard {
  background: #444a50;
  padding-top:20px;
  border: 1px solid #495058;
  width:952px;
  position:relative;
  left:-1px;
}

#body.dashboard img#content_top_edit_form {
  display:none;
}

#body.dashboard p.loading {
  text-align:center;
  font-size:2em;
  color:#C4CDD6;
  line-height:2em;
  margin:0.5em;
}

#body.dashboard p.loading img {
  vertical-align:middle;
  margin: 0 0.25em;
}

#body.dashboard  p {
  color:#C4CDD6;
}


#sidebar,
#content {
  padding:20px;
  padding-top:0;
}

#content {
  padding-right:0;
}

#sidebar {
  padding-left:0;
}

#content_nav {
  background: #fff url("<%= image_path('atreides/admin/content_top_edit_form.png') %>") 10% 0 no-repeat;
  -moz-border-radius: 5px;
  -webkit-border-radius: 5px;
  border-radius: 5px;
  border:1px solid #ddd;
  padding:5px 10px;
  margin-bottom:2em;
}

#content_nav ul {
  background: none;
  width:520px;
  height:85px;
  margin:0 auto;
  padding:0;
}

#content_nav li {
  list-style:none;
  float:left;
  margin-right:5px;
  width:70px;
}

#content_nav li a {
  display:block;
  padding-top:65px;
  text-align:center;
  text-transform:uppercase;
  color:#666;
  text-decoration:none;
  background: transparent url("<%= image_path('atreides/admin/new_content_icons.png') %>") -10px -8px no-repeat;
}

#content_nav li.photos a {
  background-position: -83px -8px;
}

#content_nav li.videos a {
  background-position: -158px -8px;
}

#content_nav li.features a {
  background-position: -568px -8px;
}

#content_nav li.contributors a {
  background-position: -648px -8px;
}

#content_nav li.archives a {
  background-position: -265px -8px;
}

#content_nav li.product a {
  background-position: -415px -8px;
}

#content_nav li.comments a {
  background-position: -343px -8px;
}

#content_nav li.store a {
  background-position: -415px -8px;
}

#content_nav li.text a {
  background-position: -492px -8px;
}

#content_nav li.pages a {
  background-position: -10px -8px;
}

#content_nav li:last-child {
  margin-right:0;
}

/*
 * Sidebar
 *
 */

#sidebar {
}

.dashboard #content h2,
#sidebar h2 a {
  color:#fff;
  text-decoration:none;
  display:block;
}

#sidebar h2 a span {
  display:none;
}

#sidebar h2 a:hover span {
  display:inline;
  float:right;
  text-transform:uppercase;
  font-size:0.5em;
  line-height:3em;
}

#sidebar ul {
  list-style:none;
  margin:0;
  padding: 0 1.5em ;
}

#sidebar li.divider {
  height:1.5em;
  background: transparent url("<%= image_path('atreides/admin/form_sidebar_divider.png') %>") 0 50% repeat-x;
}

#sidebar li.new-content a {
  padding:0 0.5em;
  display:block;
  font-size:1.5em;
  line-height:2.2em;
  -moz-border-radius: 5px;
  -webkit-border-radius: 5px;
  border-radius: 5px;
  background:#ddd url("<%= image_path('atreides/admin/add.png') %>") 10px 55% no-repeat;
  border:1px solid #bbb;
  text-decoration:none;
  color:#666;
  font-weight:bold;
  text-indent:25px;
}

#sidebar li.new-content a:hover {
  background-color:#ccc;
  border:1px solid #aaa;
}

.dashboard #sidebar h2 {
  margin-bottom:0
}

.dashboard #sidebar h2 a,
.dashboard #sidebar li.new-content a {
  color: #C4CDD6;
  background-color:inherit;
  border:1px solid transparent;
  padding:0 0.5em;
  -moz-border-radius: 5px;
  -webkit-border-radius: 5px;
  border-radius: 5px;
  text-shadow:0 1px 0 #000000;
}

.dashboard #sidebar li.new-content a {
  background-image:url("<%= image_path('atreides/admin/add_dashboard.png') %>");
}

.dashboard #sidebar h2 a:hover,
.dashboard #sidebar li.new-content a:hover {
  border-color:#495058;
  background-color:#282C30;
}

.dashboard #sidebar img {
  vertical-align:middle;
}

.dashboard #sidebar li,
.dashboard #sidebar a {
  color:#C4CDD6;
  text-decoration:none;
}

.dashboard #sidebar a:hover {
  text-decoration:underline;
}

.dashboard #sidebar li.divider {
  background-image: url("<%= image_path('atreides/admin/dashboard_nav_border.png') %>");
}

/*
 * Dashboard
 *
 */

#content .chart h3 {
  color:#fff;
  margin:0;
  text-indent:1em;
  line-height:2em;
}

#content .chart h3 span {
  float:right;
  margin-right:1em;
  font-size:1.2em;
}

.chart {
  border:1px solid #495058;
  background:#282C30;
  -moz-border-radius: 5px;
  -webkit-border-radius: 5px;
  border-radius: 5px;
  margin-bottom:2em;
}

.chart table {
  width:95%;
  margin:0 auto 1.5em auto;
}

.chart table thead th {
  color:#eee;
  font-weight:bold;
}

.chart table td {
  background:#eee;
}

.chart.top_referrers,
.chart.top_landing_pages,
.chart.tweet-reach,
.chart.tweet-exposure,
.chart.fb-page-views,
.chart.fb-page-likes {
  width:48%;
  float:left;
}

.chart.top_referrers table,
.chart.top_landing_pages table {
  width:90%;
}

.chart.top_referrers,
.chart.tweet-reach,
.chart.fb-page-views {
  margin-right:3%;
}

.chart.countries_week {
  clear:both;
}

.graph_canvas {
  height: 240px;
  width: 100%;
}

#countries_pie {
  height: 360px;
  width: 100%;
}

/*
 * Content
 *
 */

#content h2,
#content h3 {
  font-weight:bold;
  color:#444a50;
}

.search-form input,
.search-form a {
  padding:2px;
  font-size:1.2em;
  line-height:2em;
}

.auto-hide-text input {
  color:#666;
}

/*
 * Archives
 *
 */

ul.archives {
  list-style:none;
  margin:0;
  padding: 0;
}

ul.archives li {
  float:left;
  margin: 0 10px 10px 0;
}

ul.archives a {
  width:90px;
  height:90px;
  overflow:hidden;
  float:left;
  display:block;
  text-decoration:none;
  padding:5px;
  background-color: #eee;
  background-position: 50% 50%;
}

ul.archives a .date-overlay {
  display:none;
}

ul.archives a:hover .date-overlay {
  display:block;
  width:100px;
  height:100px;
  text-align:center;
  line-height:100px;
  font-size:2em;
  color:#fff;
  font-weight:bold;
  background:#000;
  opacity:0.66;
  position:absolute;
  margin:-5px;
}

ul.archives a span {
  color:#fff;
  font-weight:bold;
  background:#000;
  line-height:1.2em;
  padding:1px;
}

.pagination a {
  color:#C4CDD6
}

.pagination .previous {
  float:left;
}

.pagination .next {
  float:right;
}

.pagination .more a {
  display:block;
  text-align:center;
  border:1px solid #495058;
  -moz-border-radius: 5px;
  -webkit-border-radius: 5px;
  border-radius: 5px;
  margin-bottom:2em;
  width:650px;
  padding:0.5em 0;
  text-decoration:none;
  font-size:1.2em;
}

.pagination .more a:hover {
  background:#282C30;
  text-decoration:underline;
}

/*
 * Posts
 *
 */

#map_canvas {
  width:100%;
  height:200px;
}

#content_parts_drops_list li,
#content_parts_drops_list span {
  float:none !important;
  display:block;
  cursor:move;
}

#content_parts_drops_list span {
  margin: 5px 2px;
  padding-left:40px;
  line-height:34px;
  background: transparent url("<%= image_path('atreides/admin/content_part_icons.png') %>") 0 0 no-repeat;
}

#content_part_photos span {
  background-position: 0 -32px;
}

#content_part_videos span {
  background-position: 0 -64px;
}

#content_parts_drops_list .draggable-sshot {
  opacity:0.6;
}

#content_part_loading,
#content_part_drop,
#content_parts_container .empty-message {
  border-radius: 5px;
  background: #f5f5f5 url("<%= image_path('atreides/admin/large_loading.gif') %>") 28% 50% no-repeat;
  border: 3px solid #686868;
  width:auto;
  text-align:center;
  text-indent:45px;
  line-height:3em;
  font-size:2em;
  font-weight:bold;
  color:#686868;
  margin:0 auto 1em auto;
  display:none;
  text-shadow: 0 1px 0 #fff;
  box-shadow:inset 0 1px 5px #666;
  -moz-box-shadow:inset 0 1px 5px #666;
  -webkit-box-shadow:inset 0 1px 5px #666;
}

#content_part_drop,
#content_parts_container .empty-message {
  background-image: url("<%= image_path('atreides/admin/content_part_drop_target.png') %>");
  background-position:33% 50%;
}

#content_parts_container .empty-message {
  background-position:20px 50%;
  display:block;
}

#content_parts_container.droppable-active #content_part_drop,
#content_parts_container.loading #content_part_loading {
  display:block;
}

#content_parts_container.droppable-active .empty-message,
#content_parts_container.loading .empty-message {
  display:none;
}


#content_parts_container .part-container {
  background:#fff;
  border-radius:7px;
}

#content_parts_container .part-container.ui-sortable-helper {
  border: 3px solid #aaa;
  background:#f5f5f5;
  position:relative;
  left:-3px;
  top:-3px;
}

#content_parts_container.droppable-active .part-container,
#content_parts_container.loading .part-container {
  background:none;
}

#content_parts_container .part-drag-handle,
#content_parts_container .part-delete-btn {
  float:right;
  margin:0;
  width:18px;
  height:18px;
  margin-top:-18px;
  position:relative;
  top:20px;
  left:-20px;
  display:none;
}

#content_parts_container .part-drag-handle {
  cursor:move;
  left:-40px;
}

#content_parts_container fieldset:hover .part-drag-handle,
#content_parts_container fieldset:hover .part-delete-btn {
  display:block;
}

#content_parts_container .ui-state-highlight {
  height:5px;
  line-height:5px;
  border-width:2px;
  border-radius:5px;
  -moz-border-radius:5px;
  -webkit-border-radius:5px;
  text-align:center;

}

/*
 * Orders
 *
 */


td.items_field a {
  display:block;
  float:left;
  margin:5px;
}

.order-form .items li {
  font-size:1.25em;
}

.order-form .items li img {
  vertical-align:middle;
}


/*
 * Pages
 *
 */

.atreides_pages_table img.page_icon {
  float:left;
  margin-right:0.5em;
  vertical-align:middle;
}

.atreides_pages_table .collapsed,
.atreides_pages_table .expanded {
  display:block;
  float:left;
  height:17px;
  width:17px;
}

.atreides_pages_table .collapsed a,
.atreides_pages_table .expanded a {
  display:block;
  float:left;
  margin-right:0.5em;
  height:17px;
  width:17px;
  margin-top:8px;
  background: transparent url("<%= image_path('atreides/admin/expand_collapse.png') %>") 0 0 no-repeat
}

.atreides_pages_table .expanded a {
  background-position: 0 100%;
}

.atreides_pages_table .page_column {
  line-height:30px;
}

#sidebar #filter-list ul {
  padding:0;
}

/*
 * Features
 *
 */

#feature_post_browser {
  background:#eee;
  border:1px solid #ccc;
  -moz-border-radius: 5px;
  padding:10px 11px;
  -webkit-box-shadow: #000 0px 1px 3px;
  -moz-box-shadow: #999 0px 2px 4px;
  box-shadow: #000 0px 1px 3px;
}

#feature_post_browser .close a {
  line-height:2em;
  float:right;
  background: transparent url("<%= image_path('atreides/admin/remove.png') %>") 50% 50% no-repeat;
  text-indent:-999px;
  overflow:hidden;
  display:block;
  width:22px;
  height:22px;
}

#feature_post_browser label {
  width:auto;
  margin-bottom:0.5em;
}

#feature_post_browser li {
  float:left;
  width:auto;
  margin-bottom:10px;
}

#feature_post_browser li:nth-child(6n+6) {
  margin-right:0;
}

#feature_post_browser li a {
  width:82px;
  height:82px;
  font-size:0.9em;
}

#feature_post_browser .date-overlay {
  width:92px;
  height:92px;
}

#feature_post_browser .more a {
  width:99%;
  border:none;
  color:#333;
  font-weight:bold;
  margin:0;
}

#feature_post_browser .more a:hover {
  color:#333;
  background:#ddd;
}

#feature_photo_ul li {
  width:auto;
}

/*
 * Forms
 *
 */

form.formtastic fieldset {
  margin-left:10px;
  width:100%;
}

#form_sidebar fieldset.inputs {
  width: 230px;
}

form .sidebar fieldset {
  margin-left:20px;
}

form.formtastic .sidebar fieldset ol li label {
  width:100%;
}

form.formtastic .sidebar li li label {
  font-size:1.1em;
  line-height:1.5em;
}

form .sidebar li {
  padding-top:1.5em;
  background: transparent url("<%= image_path('atreides/admin/form_sidebar_divider.png') %>") 0 0 repeat-x;
}

form .sidebar li li {
  padding:0;
  float:left;
  background:none;
  width:auto;
}

form .sidebar ol > li:first-child {
  padding-top:0;
  background: none;
}

form .sidebar textarea {
  width:100%;
  height:auto;
}

form .sidebar textarea[disabled] {
  background:none;
}

form.formtastic fieldset ol li {
  width:95%;
}

form.formtastic fieldset ol li label {
  float:none;
  font-weight:normal;
  font-size:1.25em;
  color:#666;
  width:50%;
}

form.formtastic fieldset ol li.string input,
form.formtastic fieldset ol li.text textarea,
form.formtastic fieldset ol li.email input,
form.formtastic fieldset ol li.url input,
form.formtastic fieldset ol li.password input {
  width:100%;
  padding:2px;
  font-size:1.2em;
}

form.formtastic fieldset ol li.text span.mceEditor {
  width:100%;
}

form.formtastic fieldset ol li.boolean input {
  top:0;
}

form.formtastic fieldset ol li.boolean label {
  padding-left:0;
}


form.formtastic fieldset ol li.url input.title,
form.formtastic fieldset ol li.string input.title,
form.formtastic fieldset ol li.email input.title,
form.formtastic fieldset ol li.password input.title {
  padding:5px;
  font-size:2em;
}

form.formtastic fieldset ol li p.inline-hints {
  margin-left:0;
}

form.formtastic fieldset ol li.date-picker input[type=text] {
  width:92%;
  background: #fff url("<%= image_path('atreides/admin/calendar.png') %>") 4px 2px no-repeat;
  text-indent:20px;
}

.common-tags label {
  margin-bottom:0.5em;
}

.jq_tags_editor {
  min-height: 30px;
  padding: 5px 5px 10px 5px;
  cursor:test;
  border:1px solid #bbb;
  background: url("<%= image_path('atreides/admin/input_bg.gif') %>") repeat-x scroll 0 0 #FFFFFF;
}

.jq_tags_editor input[type="text"] {
  border: none;
}

.jq_tags_token,
.tag-picker .jq_tags_token {
  -moz-border-radius: 2px;
  -webkit-border-radius: 2px;
  background: #c7da76 !important;
  border: solid 1px #b7c963;
  color: #111;
  font-weight:normal;
  float: left;
  margin: 0 5px 5px 0;
  padding: 0 5px;
  line-height:1.75em;
  width:auto;
}

.jq_tags_token a,
.common-tags .jq_tags_token {
  color: #749440;
  font-weight: bold;
  margin-left: 7px;
  text-decoration:none;
}

.jq_tags_editor input,
.jq_tags_editor input:not([type="checkbox"]):not([type="radio"]):not([type="file"])  {
  background:none;
  border:none;
  box-shadow:none;
}

li.machine_tag,
#content_parts_drops_list li {
  -moz-border-radius: 3px;
  -webkit-border-radius: 3px;
  border-radius: 3px;
  color: #111;
  font-weight:normal;
  float: left;
  margin: 0 5px 5px 0;
  padding: 0 5px;
  line-height:2.25em;
  width:auto;
  border:1px solid #ccc;
  color:#666;
  background-color:#f5f5f5;
}

li.machine_tag:hover,
li.machine_tag.selected,
#content_parts_drops_list li:hover {
  background-color:#ddd;
}

form.formtastic .machine_tag input,
form.formtastic .machine_tag a {
  vertical-align:top;
  margin: 5px;
}

form.formtastic .machine_tag a,
form.formtastic .machine_tag span {
  margin-left: 0.5em;
}

form.formtastic .machine_tag a,
form.formtastic .machine_tag span {
  margin-left: 0.5em;
}

form.formtastic fieldset ol li.price span {
  float:left;
  font-size:1.5em;
  font-weight:bold;
  color:#666;
  line-height:1.3em;
  padding-right:0.25em;
}

form.formtastic fieldset ol li.price input {
  width:85%;
}

form.formtastic fieldset ol li.underline span {
  float:left;
