  border:1px solid #000;
}

.media-list ul li a {
  display:none;
}

.media-list ul li:hover a {
  display:block;
  position:absolute;
  margin:-10px 0 0 -10px;
  padding:1px;
  height:18px;
  width:18px;
  background:#f5f5f5;
  border:1px solid #aaa;
  -moz-border-radius: 2px;
  -webkit-border-radius: 2px;
  border-radius: 2px;
}

.media-list ul li textarea {
