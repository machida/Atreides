}

.photos-display-type span {
  display:block;
  float:left;
  width:42%;
  margin-top:4px;
  margin-right:4%;
  padding: 0 0.25em;
  border:1px solid #ccc;
  color:#666;
  background:#f5f5f5;
  line-height:45px;
  font-size:1.1em;
  -moz-border-radius: 3px;
  -webkit-border-radius: 3px;
  border-radius: 3px;
}

.photos-display-type span.selected {
