  font-size:1em;
  line-height:1.5em;
  height:4.5em;
  padding-left: 5px;
}

form.formtastic .videos-list li textarea {
  margin-top:5px;
  width:180px;
}

form.formtastic .videos-list li,
form.formtastic .photos-list li {
  width:auto;
}

.flash-replaced .alt {
  display:none;
}

form.formtastic fieldset ol li.add-inline-photo {
