  overflow:hidden;
}

.photos-choose a:hover {
  background-position: 0 50%;
}

.photos-choose a:active {
  background-position: 0 100%;
}

form.formtastic fieldset ol li.video-url input {
  width:65%;
}

form.formtastic fieldset ol li.video-url a {
  float:right;
  display:block;
  width:84px;
